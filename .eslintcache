=> import('./pages/TeamList'));\nconst Team = React.lazy(() => import('./pages/Team'));\nconst MatchList = React.lazy(() => import('./pages/MatchList'));\nconst Match = React.lazy(() => import('./pages/Match'));\nconst PlayerList = React.lazy(() => import('./pages/PlayerList'));\nconst Player = React.lazy(() => import('./pages/Player'));\nconst AddPlayer = React.lazy(() => import('./pages/AddPlayer'));\nconst AddTeam = React.lazy(() => import('./pages/AddTeam'));\nconst AddMatch = React.lazy(() => import('./pages/AddMatch'));\nconst Winner = React.lazy(() => import('./pages/WinnerPage'));\n\n\n\n\nfunction App() {\n  const currentToken = localStorage.getItem(\"tokens\");\n  const currentUserId = localStorage.getItem(\"user\"); ;\nconst [token,setToken] = useState(currentToken);\nconst [userId,setUserId] = useState(currentUserId);\n  const login = (token, userId, tokenExpiration) => {\n    localStorage.setItem(\"tokens\",token);\n    localStorage.setItem(\"user\",userId);\n    setToken(token);\n    setUserId(userId);\n  };\n\n  const logout = () => {\n     setToken(null);\n     setUserId(null)\n  };\n\n\n  return (\n    <div className='App'>\n      <ErrorBoundary>\n        <Suspense fallback={<Loader status={true} />}>\n          <Auth.Provider value={{\n              token: token,\n              userId: userId,\n              login: login,\n              logout: logout\n            }}>\n          <Router>\n            <Header />\n            {!token && <Route exact path='/' component={Login} />}\n            {!token && <Route exact path='/signup' component={signup} />}\n            {token && <Route exact path='/' component={Home} />}\n            <Route exact path='/teams' component={TeamList} />\n            <Route exact path='/team/info/:teamId' component={Team} />\n            <Route exact path='/team/add' component={AddTeam} />\n            <Route exact path='/matches/' component={MatchList} />\n            <Route exact path='/match/info/:matchId' component={Match} />\n            <Route exact path='/match/add' component={AddMatch} />\n            <Route exact path='/players/' component={PlayerList} />\n            <Route exact path='/player/info/:playerId' component={Player} />\n            <Route exact path='/player/add' component={AddPlayer} />\n            <Route exact path='/winner' component={Winner} />\n          </Router>\n          </Auth.Provider>\n        </Suspense>\n      </ErrorBoundary>\n    </div>\n      );\n}\n\nexport default App;\n","/Users/saravanan/scorer/src/reducers/reducers.js",[],"/Users/saravanan/scorer/src/utils/ErrorBoundary.js",[],"/Users/saravanan/scorer/src/reducers/playerreducer.js",[],"/Users/saravanan/scorer/src/reducers/matchreducer.js",[],"/Users/saravanan/scorer/src/reducers/teamreducer.js",[],"/Users/saravanan/scorer/src/pages/signuppage.js",["154"],"import React, { useState, useContext } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport Auth from \"../context/auth-context\";\nimport { signup } from \"../services/loginservices\";\n\nconst Signuppage = (props) => {\n  const context = useContext(Auth);\n  const [Login, setLogin] = useState({});\n  const history = useHistory();\n  const redirectTo = (route) => {\n    history.push(route);\n  };\n  const handleForm = (value, key) => {\n    setLogin({ ...Login, [key]: value });\n    console.log(Login);\n  };\n  const signupHandler = () => {\n    const email = Login.email;\n    const password = Login.password;\n    signup(email, password)\n      .then((res) => {\n        console.log(res);\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n    redirectTo(\"/\");\n  };\n\n  return (\n    <div class=\"m-4\">\n      <div\n        className=\"p-3 h5 d-flex col-md-2 mx-auto\"\n        style={{ alignItems: \"center\" }}\n      >\n        Create user\n      </div>\n      <form className=\"text-secondary\">\n        <div class=\"form-group\">\n          <label>E-mail</label>\n          <input\n            class=\"form-control\"\n            name=\"email\"\n            value={Login.email}\n            onChange={(e) => handleForm(e.target.value, e.target.name)}\n          />\n        </div>\n        <div class=\"form-group\">\n          <label>Password</label>\n          <input\n            class=\"form-control\"\n            name=\"password\"\n            value={Login.password}\n            onChange={(e) => handleForm(e.target.value, e.target.name)}\n          />\n        </div>\n        <div className=\"form-actions mt-2\">\n          <button\n            class=\"btn btn-md btn-primary w-100\"\n            type=\"button\"\n            onClick={signupHandler}\n          >\n            Create\n          </button>\n        </div>\n      </form>\n    </div>\n  );\n};\n\nexport default Signuppage;\n","/Users/saravanan/scorer/src/pages/login.js",["155"],"import React, { useState, useContext } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport Auth from \"../context/auth-context\";\nimport { login } from \"../services/loginservices\";\n\nconst Login = (props) => {\n  const context = useContext(Auth);\n  const [Login, setLogin] = useState({});\n  const history = useHistory();\n  const redirectTo = (route) => {\n    history.push(route);\n  };\n  const handleForm = (value, key) => {\n    setLogin({ ...Login, [key]: value });\n    console.log(Login);\n  };\n  const loginHandler = () => {\n    const email = Login.email;\n    const password = Login.password;\n    login(email, password)\n      .then((res) => {\n        console.log(res.data.data);\n        context.login(\n          res.data.data.login.token,\n          res.data.data.login.userId,\n          res.data.data.login.tokenExpiration\n        );\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  };\n  return (\n    <div class=\"m-4\">\n      <div\n        className=\"p-3 h5 d-flex col-md-2 mx-auto\"\n        style={{ alignItems: \"center\" }}\n      >\n        Login\n      </div>\n      <form className=\"text-secondary\">\n        <div class=\"form-group\">\n          <label>E-mail</label>\n          <input\n            class=\"form-control\"\n            name=\"email\"\n            value={Login.email}\n            onChange={(e) => handleForm(e.target.value, e.target.name)}\n          />\n        </div>\n        <div class=\"form-group\">\n          <label>Password</label>\n          <input\n            class=\"form-control\"\n            name=\"password\"\n            value={Login.password}\n            onChange={(e) => handleForm(e.target.value, e.target.name)}\n          />\n        </div>\n        <div className=\"form-actions mb-5 mt-2\">\n          <button\n            class=\"btn btn-md btn-primary w-100\"\n            type=\"button\"\n            onClick={loginHandler}\n          >\n            Login\n          </button>\n        </div>\n        Create an account?\n        <a\n          href=\"\"\n          class=\" w-30 ml-3\"\n          type=\"button\"\n          onClick={() => {\n            redirectTo(\"/signup\");\n          }}\n        >\n          signup\n        </a>\n      </form>\n    </div>\n  );\n};\n\nexport default Login;\n","/Users/saravanan/scorer/src/pages/WinnerPage.js",["156"],"import React from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport trophy from \"../assets/trophy.png\";\nimport close from \"../assets/close.png\";\n\nconst WinnerPage = ()=>{\n     const history = useHistory();\n    \n     const redirectTo = (route) => {\n        history.push(route);\n      };\n    \nreturn(\n    <div>\n        <h1><center>Congratulations</center></h1>\n   <div class=\"float-right\" style={{marginRightt:\"30px\"}} onClick={() => redirectTo(`/`)}>\n             <img  alt=\"\" src={close} width=\"30px\" alt=\"\" style={{ cursor: \"pointer\" }}/>    \n            </div>\n    <img class=\"rounded mx-auto d-block\" alt=\"\" height=\"240px\" src={trophy}/>\n    <div class=\"text-center\"><h3>Mumbai Indi